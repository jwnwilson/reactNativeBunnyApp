{
  "expo": {
    "name": "React Native Bunny App",
    "slug": "react-native-bunny-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bunny_icon.jpg",
    "splash": {
      "image": "./assets/images/home_bunny.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.noelwilson.bunnyapp"
    },
    "android": {
      "package": "com.noelwilson.bunnyapp"
    }
  }
}